<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <data></data>

<FrameLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.RgbFragment">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <include layout="@layout/module_luminance_zone"
            app:layout_constraintTop_toBottomOf="@id/all_on_off"
            app:layout_constraintBottom_toBottomOf="parent"
            />
           <com.lin.voltrfremoteadaptorandroid.module.AllOnOffModule
               android:id="@+id/all_on_off"
               android:layout_width="match_parent"
               android:layout_height="0dp"
               tools:ignore="MissingConstraints"
               app:layout_constraintTop_toTopOf="parent"
               />


    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>
</layout>